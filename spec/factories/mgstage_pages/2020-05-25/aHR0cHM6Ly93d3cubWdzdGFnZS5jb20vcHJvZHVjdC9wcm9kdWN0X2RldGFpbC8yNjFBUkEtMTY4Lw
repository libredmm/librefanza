                    $("a.button_postback").bind("click",function(e) {
                        e.preventDefault();
                        $id   = $(this).attr("id");
                        $attr = $(this).attr("for");
                        $form = $(this).attr("href");
                        $($form+" #__EVENTTARGET").val($id);
                        $($form+" #__EVENTARGUMENT").val($attr);
                        $($form).submit()
                    });

<style>
    .how_to_use_point .ui-dialog-titlebar {
        display:none;
    }
</style>

<script>
    $(function() {
        $('.point_use_anchor').on('click', function() {
            $("#point_use_img").attr('src', IMG_URL_NEW + '/point_use.jpg')
            $("#point_use_dialog").dialog("open");
        });
    });

    (function() {
        loadJqueryUiScriptAndCssAfterLoadedHead(function() {
            $("#point_use_dialog").dialog({
                dialogClass: "how_to_use_point",
                modal: true,
                autoOpen: false,
                height: 600,
                width: 850,
                buttons: {
                    '閉じる': function() {
                        $(this).dialog('close');
                    }
                }
            });

            // 上書きするjqueryUIのCSSをここでロードする
            var head = document.getElementsByTagName('head');
            var link = document.createElement('link');
            link.setAttribute('rel', 'stylesheet');
            link.setAttribute('href', '/style/mgs/pc/mypage_affiliate_jquery_ui.css');
            head[0].appendChild(link);
        });
    })();

    function loadJqueryUiScriptAndCssAfterLoadedHead(callback) {
        var done = false;
        var head = document.getElementsByTagName('head');
        var script = document.createElement('script');
        script.setAttribute('src', '//code.jquery.com/ui/1.11.4/jquery-ui.js');
        var link = document.createElement('link');
        link.setAttribute('rel', 'stylesheet');
        link.setAttribute('href', '//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css');

        head[0].appendChild(link);
        head[0].appendChild(script);

        script.onload = script.onreadystatechange = function() {
            if(!done && (!this.readyState || this.readyState === "loaded" || this.readyState === "complete")) {
                done = true;
                callback();

                script.onload = script.onreadystatechange = null;
                if(head && script.parentNode) {
                    head.removeChild(script);
                }
            }
        };
    }
</script>



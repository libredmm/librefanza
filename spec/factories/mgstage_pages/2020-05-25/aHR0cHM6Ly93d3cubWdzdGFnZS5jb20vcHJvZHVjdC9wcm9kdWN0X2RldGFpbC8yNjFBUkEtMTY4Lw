<script data-footer_banner="1" id="script-footer_banner">
    $(function () {
        var rand = Math.floor(Math.random() * $('#script-footer_banner').data('footer_banner'));
        const display_image_id = 'footer_banner-' + rand;
        $('[id^=footer_banner-]').each(function () {
            if (display_image_id === $(this).attr('id')) {
                $(this).attr('style', 'display: block;');
            } else {
                $(this).attr('style', 'display: none;');
            }
        });
    });
</script>



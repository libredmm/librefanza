<script type="text/javascript">
    var is_sales_start = '1';

    
    var bonus_flg;
    function setBonusFlg (n) {
        bonus_flg = n;
    }

    $(function() {
        $("#st_detail").ready(function(){
            if ( $("input[name='price']:checked").val().split(",")[0] == 'streaming') {
                $("#st_detail").css("display","inline-table");
                $("#st_detail_point").css("display","block");
            } else {
                $("#st_detail").css("display","none");
                $("#st_detail_point").css("display","none");
            }
        });

        $("#dl_detail").ready(function(){
            if ( $("input[name='price']:checked").val().split(",")[0] == 'download') {
                $("#dl_detail").css("display","inline-table");
                $("#dl_detail_point").css("display","block");
            } else {
                $("#dl_detail").css("display","none");
                $("#dl_detail_point").css("display","none");
            }
        });

        $("#dlh_detail").ready(function(){
            if ( $("input[name='price']:checked").val().split(",")[0] == 'download_hd') {
                $("#dlh_detail").css("display","inline-table");
                $("#dlh_detail_point").css("display","block");
            } else {
                $("#dlh_detail").css("display","none");
                $("#dlh_detail_point").css("display","none");
            }
        });

        $("#st_vr_detail").ready(function(){
            if ( $("input[name='price']:checked").val().split(",")[0] == 'streaming_vr') {
                $("#st_vr_detail").css("display","inline-table");
                $("#st_vr_detail_point").css("display","block");
            } else {
                $("#st_vr_detail").css("display","none");
                $("#st_vr_detail_point").css("display","none");
            }
        });

        $("#dl_vr_detail").ready(function(){
            if ( $("input[name='price']:checked").val().split(",")[0] == 'download_vr') {
                $("#dl_vr_detail").css("display","inline-table");
                $("#dl_vr_detail_point").css("display","block");
            } else {
                $("#dl_vr_detail").css("display","none");
                $("#dl_vr_detail_point").css("display","none");
            }
        });

        $("#dlh_vr_detail").ready(function(){
            if ( $("input[name='price']:checked").val().split(",")[0] == 'download_hd_vr') {
                $("#dlh_vr_detail").css("display","inline-table");
                $("#dlh_vr_detail_point").css("display","block");
            } else {
                $("#dlh_vr_detail").css("display","none");
                $("#dlh_vr_detail_point").css("display","none");
            }
        });

        $("input[name=price]:radio").click( function() {
            var argument =  $( this ).val().split(",");
            var target   =  argument[0];
            var sku      =  argument[1];
            $(".price_selection").removeClass("select");
            $(".sale_time").removeClass("select");
            $("table#"+target).addClass("select");
    
        if (isDiscountedViewType(target)) {
            
                $("#ch_price").html($("strong#"+target+"_sale_price").html());
                $("#ch_point").html($("strong#"+target+"_sale_point").html());
                $("#us_price").html($("strong#"+target+"_price").html());
                /*$(".sale_info").show();*/
            
        } else {
            
                $("#ch_price").html($("strong#"+target+"_price").html());
                $("#ch_point").html($("strong#"+target+"_point").html());
                $("#us_price").html($("strong#"+target+"_fixed_price").html());
                $(".sale_info").hide();
            
        }
    
            if(target == "streaming"){
                $("#st_detail").css("display","inline-table");
                $("#st_detail_point").css("display","block");
                $("#dl_detail").css("display","none");
                $("#dl_detail_point").css("display","none");
                $("#dlh_detail").css("display","none");
                $("#dlh_detail_point").css("display","none");
                $("#st_vr_detail").css("display","none");
                $("#st_vr_detail_point").css("display","none");
                $("#dl_vr_detail").css("display","none");
                $("#dl_vr_detail_point").css("display","none");
                $("#dlh_vr_detail").css("display","none");
                $("#dlh_vr_detail_point").css("display","none");
            }else if (target == "download") {
                $("#dl_detail").css("display", "inline-table");
                $("#dl_detail_point").css("display", "block");
                $("#st_detail").css("display", "none");
                $("#st_detail_point").css("display", "none");
                $("#dlh_detail").css("display", "none");
                $("#dlh_detail_point").css("display", "none");
                $("#st_vr_detail").css("display","inone");
                $("#st_vr_detail_point").css("display","none");
                $("#dl_vr_detail").css("display","none");
                $("#dl_vr_detail_point").css("display","none");
                $("#dlh_vr_detail").css("display","none");
                $("#dlh_vr_detail_point").css("display","none");
            }else if (target == "download_hd") {
                $("#dlh_detail").css("display","inline-table");
                $("#dlh_detail_point").css("display","block");
                $("#dl_detail").css("display","none");
                $("#dl_detail_point").css("display","none");
                $("#st_detail").css("display","none");
                $("#st_detail_point").css("display","none");
                $("#dl_vr_detail").css("display","none");
                $("#dl_vr_detail_point").css("display","none");
                $("#st_vr_detail").css("display","none");
                $("#st_vr_detail_point").css("display","none");
                $("#dlh_vr_detail").css("display","none");
                $("#dlh_vr_detail_point").css("display","none");
            }else if (target == "streaming_vr") {
                $("#dl_detail").css("display", "none");
                $("#dl_detail_point").css("display", "none");
                $("#st_detail").css("display", "none");
                $("#st_detail_point").css("display", "none");
                $("#dlh_detail").css("display", "none");
                $("#dlh_detail_point").css("display", "none");
                $("#dl_vr_detail").css("display","none");
                $("#dl_vr_detail_point").css("display","none");
                $("#st_vr_detail").css("display","inline-table");
                $("#st_vr_detail_point").css("display","block");
                $("#dlh_vr_detail").css("display","none");
                $("#dlh_vr_detail_point").css("display","none");
            }else if (target == "download_vr") {
                $("#dl_detail").css("display", "none");
                $("#dl_detail_point").css("display", "none");
                $("#st_detail").css("display", "none");
                $("#st_detail_point").css("display", "none");
                $("#dlh_detail").css("display", "none");
                $("#dlh_detail_point").css("display", "none");
                $("#dl_vr_detail").css("display","inline-table");
                $("#dl_vr_detail_point").css("display","block");
                $("#st_vr_detail").css("display","none");
                $("#st_vr_detail_point").css("display","none");
                $("#dlh_vr_detail").css("display","none");
                $("#dlh_vr_detail_point").css("display","none");
            }else if (target == "download_hd_vr") {
                $("#dl_detail").css("display", "none");
                $("#dl_detail_point").css("display", "none");
                $("#st_detail").css("display", "none");
                $("#st_detail_point").css("display", "none");
                $("#dlh_detail").css("display", "none");
                $("#dlh_detail_point").css("display", "none");
                $("#dl_vr_detail").css("display","none");
                $("#dl_vr_detail_point").css("display","none");
                $("#st_vr_detail").css("display","none");
                $("#st_vr_detail_point").css("display","none");
                $("#dlh_vr_detail").css("display","inline-table");
                $("#dlh_vr_detail_point").css("display","block");
            }

            if (target == "download") {
                var type = "download";
            } else {
                var type = target;
            }

            if(is_sales_start){
                var linkurl = "/cart/cart/add/" + type + "/" + sku;
                $("#dldp").attr({href:linkurl});
            }
        });

        $("#dldp").click( function() {
            if (!$("input:radio[name='price']:checked").val()) {
                alert('購入する商品を選択してください。');
                return false;
            }
        });

        // 関連女優作品の表示切替
        $(".push_connect_list h4 a").click( function() {
            var index = $(".push_connect_list h4 a").index(this);

            $(".push_connect_list .rank_list div.connect_cover").css("display", "none");
            $(".push_connect_list .rank_list div.connect_cover").eq(index).css("display", "");

            $(".push_connect_list .rank_list ul").css("display", "none");
            $(".push_connect_list .rank_list ul").eq(index).css("display", "");
        });
        $("input[name='price']:checked").get(0).click();
    });
    
    $(window).load(function () {
        $("#ModalBackground").click(function() {
            SamplePlayerWindow.close();
        });
    });
</script>

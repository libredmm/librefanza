<script>
$(function(){
	$('#sheader_button').bind("click", function(){
		if( $('#idsheader_nav').css('display') == "block" ){
			$('#idsheader_nav').slideUp(200);
			$('#sheader_button>.icon').removeClass('icon_up');
			$('#sheader_button>.icon').addClass('icon_down');
			$('#sheader_button>.button_text').text('フィルタ');
			$('.selected_genre_name').css('display','inline-block');
		}else{
			$('#idsheader_nav').slideDown(200);
			$('#sheader_button>.icon').removeClass('icon_down');
			$('#sheader_button>.icon').addClass('icon_up');
			$('#sheader_button>.button_text').text('閉じる');
			$('.selected_genre_name').css('display','none');
		}
	});
});
</script>
<script type="text/javascript">
$(function() {
	var topBtn = $('#entertop');
	topBtn.hide();
	$(window).scroll(function () {
		if ($(this).scrollTop() > 100) {
			topBtn.fadeIn();
		} else {
			topBtn.fadeOut();
		}
	});
    topBtn.click(function () {
		$('body,html').animate({
			scrollTop: 0
		}, 500);
		return false;
    });
});
</script>

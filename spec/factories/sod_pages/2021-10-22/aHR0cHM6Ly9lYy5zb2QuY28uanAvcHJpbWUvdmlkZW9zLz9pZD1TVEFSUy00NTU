function check_address(_sp_dvd_id){
	$('#address_check_popup_area').fadeIn();
	$('#address_check_popup_cover').fadeIn();
	$('#address_check_form').find('input[name="item"]').val(_sp_dvd_id);
	$('#address_check_form').find('input[name="product"]').val(_sp_dvd_id);
	return false;
}
$(function(){
	$('#address_check_popup_cover , #address_check_popup_area .popup_close_btn, #address_check_popup_area .popup_close').click(function(){
		$('#address_check_popup_area').fadeOut();
		$('#address_check_popup_cover').fadeOut();
	});
});

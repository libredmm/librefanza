<script>
	$(function(){
		function change_select_item(num){
			// 特典ボタンをすべて隠す
			$('.special_gift_buy_btn_set').css('display','none');
			$('.no_special_gift_buy_btn').css('display','none');
			//・選択された特典ボタンを表示する
			if( $('#special_gift_buy_btn_set_'+num).length ){
				$('#special_gift_buy_btn_set_'+num).css('display','block');
				$('.special_gift_header').css('display','block');
			}else{
				$('.no_special_gift_buy_btn').css('display','block');
				$('.special_gift_header').css('display','none');
			}
			//・「今選択されてるのは～」表示の文言を変更する
			if( $($('.special_gift_item[data-item="'+[num]+'"]')).hasClass('type_text') ){
				$('.special_gift_name').text($($('.special_gift_item[data-item="'+[num]+'"]')).find('div').text() +'付き');
			}else{
				$('.special_gift_name').text('特典'+num);
			}
		}

		$('.special_gift_item').on('click',function(){
			var $select_item = $(this);
			if( $select_item.hasClass('no_stock') == false ){
				//・選択エリアのアイコンを未選択にする
				$('.special_gift_item').removeClass('selected');
				// クリックした特典を選択状態にする
				$select_item.addClass('selected');

				if($select_item.hasClass('type_text')){
				change_select_item($select_item.attr('data-item'));
				}else{
					change_select_item($select_item.attr('data-item'),$select_item.find('div').text());
				}
			}
		});
		change_select_item( 2 );
	});
</script>
<script>
	$(function(){
		var get_review_end = '';
		$("#review_form").submit(function(){
			if ($("input[name='review_title']").val() == '') {
				alert('タイトルを入力してください');
    			return false;
			} else if ($("textarea[name='review_comment']").val() == '') {
				alert('レビュー内容を入力してください');
    			return false;
			}else {
				$("#review_form").submit();
			}
		});
		if(get_review_end == 'end'){
			$("html,body").animate({scrollTop:$('#review_body').offset().top});
			$('#review_finish_popup_area').css('display','block');
			$('#review_finish_popup_3').css('display','block');
		}
		$('.review_finish_popup_area_close_btn').click(function(){
			$('#review_finish_popup_area').fadeOut();
			$('#review_finish_popup_cover').fadeOut();
		});
	});
</script>
